              camShake: {power: 8, length: 1.5}
              particle: pt-sealFlash
              playSound: volcanoErupt
            }
            {
              optional: true
              posTile: {locations: loc-goldenTemple-buried}
              reqTileFlags: +seals2
              setTileFlags: -seals2 +seals1
              camShake: {power: 8, length: 1.2}
            }
            {
              optional: true
              posTile: {locations: loc-goldenTemple-buried}
              reqTileFlags: +seals3
              setTileFlags: -seals3 +seals2
              camShake: {power: 8, length: 1.2}
            }
          ]
        }
      }
      evt-leave
    ]
  }
