      {ref: tl-desert, reqBiomeFlags: +desert}
      {ref: tl-dry, reqBiomeFlags: +dry}
      {ref: tl-grass, reqBiomeFlags: +grass | +prehistoric}
      {ref: tl-deepSnow, reqBiomeFlags: +arctic}
