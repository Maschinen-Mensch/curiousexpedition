        optional: true
        reqBiomeFlags: +arctic
        showImage: {
          src: evt_fxt_goldenSeal_2.png
          type: day
          showMounted: true
          particles: [pt-snow-1, pt-snow-2, pt-snow-3]
          particlesChance: 0.5
        }
      }
    ]
