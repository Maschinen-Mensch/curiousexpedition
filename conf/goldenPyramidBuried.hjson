          {
            optional: true
            posTile: {
              range: ..100
              locationKnown: false
              locationFlags: +goldenSeal
              tileFlags: -mapped
            }
            setTileFlags: +mapped
            revealTiles: {radius: 1, location: true, centerCam: true}
          }
          {
            optional: true
            posTile: {
              range: ..100
              locationKnown: false
              locationFlags: +goldenSeal
              tileFlags: -mapped
            }
            setTileFlags: +mapped
            revealTiles: {radius: 1, location: true, centerCam: true}
          }
        ]
      }
      evt-leave
    ]
  }
  {
